@model EntityLayer.Concrete.WriterMessage
@{
    ViewData["Title"] = "SenderMessageDetails";
    Layout = "~/Views/Shared/_WriterLayout.cshtml";
}
<h1>Gelen Mesaj Detay</h1>
<form class="form-group">

    @Html.TextBoxFor(x => x.WriterMessageID, new { @class = "form-control" })

    @Html.Label("Gönderici Adı")
    @Html.TextBoxFor(x => x.ReciverName, new { @class = "form-control" })
    <br />
    @Html.Label("Gönderen Mail")
    @Html.TextBoxFor(x => x.Reciver, new { @class = "form-control" })
    <br />
    @Html.Label("Mesaj")
    @Html.TextAreaFor(x => x.MessageContent, 10, 3, new { @class = "form-control" })
    <br />
</form>


